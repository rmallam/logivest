# Environment Configuration for Real Estate Investment Analyzer
# Copy this file to .env and update with your API keys

# API Keys (Optional - system uses sample data if not provided)
ZILLOW_API_KEY=your_zillow_api_key_here
RENTSPYDER_API_KEY=your_rentspyder_api_key_here
GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Google Gemini AI API Key (for comprehensive property insights)
# Get your API key from: https://makersuite.google.com/app/apikey
GEMINI_API_KEY=your-gemini-api-key-here

# Server Configuration
MCP_SERVER_PORT=8000
WEB_INTERFACE_PORT=5000

# Development Settings
DEBUG_MODE=true
LOG_LEVEL=INFO

# Database Settings (for future use)
DATABASE_URL=sqlite:///real_estate.db

# External API Settings
API_TIMEOUT=30
API_RETRY_COUNT=3

# Analysis Parameters
DEFAULT_INTEREST_RATE=0.07
DEFAULT_LOAN_TERM=30
DEFAULT_EXPENSE_RATIO=0.04

# Sample Data Settings
USE_SAMPLE_DATA=true
SAMPLE_DATA_VARIATION=0.05
